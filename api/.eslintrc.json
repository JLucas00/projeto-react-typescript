{
  "env": {
      "es2021": true,
      "node": true,
      "browser": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": ["prettier", "@typescript-eslint"],
  "rules": {
      "prettier/prettier": "error"
  }
}